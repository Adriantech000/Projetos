         printf("Qual sera o valor do numero %d?:", i);
         scanf("%d", &numeros[i]);

    }
    
    i=0;
    
    do{
        
    printf("numero %d Ã© %d\n", i, numeros[i]);
        
    i++;
    }while(i<num);
    
    printf("fim");
    
