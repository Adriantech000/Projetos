#include <stdio.h>
#include <string.h>

int main(){
    
    char a1[10];
    char a2[10];
    char a3[10];
    char b1[10];
    char b2[10];
    char b3[10];
    char c1[10];
    char c2[10];
    char c3[10];
        
while(0==0){
  
  printf("%c,%c.%c\n%c,%c,%c\n%c,%c,%c", a1, a2, a3, b1, b2, b3, c1, c2, c3);
  
}

    
}
