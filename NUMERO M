#include <stdio.h>

int main() {
    
  int num1, num2, diferenca;
  
  printf("Fale o primeiro numero!\n");
  scanf("%d", &num1);
  printf("Fale o segundo numero!\n");
  scanf("%d", &num2);
  
  if(num1 > num2){
      diferenca = num1 - num2;
  }
  else{
      diferenca = num2 - num1;
  }
  
  printf("A diferença entre o maior e o menor valor é: %d\n", diferenca);
  
    return 0;
}
